# 프로덕션 환경 설정
NODE_ENV=production
PORT=3001

# 보안 설정 - 실제 배포시 변경 필요
JWT_SECRET=your-production-jwt-secret-key-here
JWT_EXPIRES_IN=24h

# 최적화 설정
CACHE_TTL=3600
COMPRESSION=true
MINIFY=true

# 로그 설정
LOG_LEVEL=info
LOG_FILE=production.log

# 보안 설정
GRAPHQL_PLAYGROUND=false
GRAPHQL_INTROSPECTION=false
SOURCE_MAP=false

# 퍼포먼스 설정
ANALYSIS_TIMEOUT=300000
MAX_ANALYSIS_FILES=1000
RATE_LIMIT_ENABLED=true